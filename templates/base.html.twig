<!DOCTYPE html>
<html lang="fr">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
				{{app_name}}
			{% endblock %}
		</title>
		{% block stylesheets %}
			{# Run `composer require symfony/webpack-encore-bundle and uncomment the following Encore helpers to start using Symfony UX #}
			{{ encore_entry_link_tags('app') }}


		{% endblock %}
		{# favicon #}
		<link rel="shortcut icon" href="#">
		{# Materia Bootstrap Theme #}
		<link rel="stylesheet" href="https://bootswatch.com/4/materia/bootstrap.min.css">
		{# Font-awesome #}
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
		{# Animate CSS #}
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" integrity="sha512-c42qTSw/wPZ3/5LBzD+Bw5f7bSF2oxou6wEb+I/lqeaKV5FDIfMvvRp772y4jcJLKuGUOpbJMdg/BTl50fJYAw==" crossorigin="anonymous"/>

	</head>
	<body id="page-top">

		<header>
			{% block navigation %}{% endblock %}
		</header>


		<!-- Page Content  -->
		{% block body %}

		
		{% endblock %}
		
		

		<div class="wrapper d-flex align-items-stretch"> 
			{% block back_navbar %}
			
			{% endblock %}
			<div id="content" class="p-4 p-md-5 pt-5 mt-5"> 
			
				{% block breadcrumbWrapper %}
					{% block breadcrumb %}{% endblock %}
				{% endblock %}
			
				{% block flashMessage %}{% endblock %}
				{% block back_main_content %}{% endblock %}

			</div>
		</div>


		{% block javascripts %}
			<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</body>
</html>
